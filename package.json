  "author": {
    "name": "Siemens",
    "email": "info@siemens.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": "^24.0.0",
    "yarn": "Please use NPM to install dependencies",
    "npm": "11"
  },
  "prettier": "@siemens/prettier-config",
  "scripts": {
